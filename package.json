  },
  "homepage": "https://github.com/joshuarwynn/joshbot#readme",
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "qs": "^6.7.0",
    "superagent": "^5.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
